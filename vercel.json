{
    "version": 2,
    "env": {
        "DB_URI": "@DB_URI",
        "FRONT_END_URL":"@FRONT_END_URL",
        "JWT_SECRET":"@JWT_SECRET",
"JWT_EXPIRES":"JWT_EXPIRES",
"FILE_UPLOAD_BUCKET":"@FILE_UPLOAD_BUCKET",
"S3_ACCESS_KEY":"@S3_ACCESS_KEY",
"S3_SECRET_KEY":"@S3_SECRET_KEY",
"PORT":"@PORT"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}